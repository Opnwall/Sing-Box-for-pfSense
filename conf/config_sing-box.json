{
  "log": {
    "disabled": false,
    "level": "error",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "/usr/local/etc/sing-box/ui",
      "default_mode": "Rule"
    },
    "cache_file": {
      "enabled": true,
      "path": "cache.db",
      "store_fakeip": true
    }
  },
  "dns": {
    "servers": [
      {
        "address": "https://1.1.1.1/dns-query",
        "tag": "remote",
        "detour": "select"
      },
      {
        "address": "https://223.5.5.5/dns-query",
        "tag": "local"
      },
      {
        "address": "8.8.8.8",
        "tag": "fallback",
        "detour": "select"
      }
    ]
  },
  "inbounds": [
    {
      "tag": "dns-in",
      "type": "direct",
      "listen": "::",
      "listen_port": 53,
      "sniff_override_destination": true,
      "network": "udp"
    },
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "127.0.0.1",
      "listen_port": 7891,
      "sniff": true,
      "udp_fragment": true,
      "domain_strategy": "prefer_ipv4"
    }
  ],
  "outbounds": [          //请根据自己的节点信息替换outbounds至route部分配置。  
    {
      "tag": "select",
      "type": "selector",
      "default": "auto",
      "outbounds": [
        "auto",
        "10",
        "11",
        "12"
      ]
    },
    {
      "type": "vmess",
      "tag": "10",
      "server": "88.88.88.88",
      "server_port": 2443,
      "uuid": "2d1fe982-1323-4023-8322-069408455f32",
      "security": "auto",
      "packet_encoding": "packetaddr",
      "transport": {
        "type": "ws",
        "path": "/2d1fe982-1323-4023-8322-069408455f32-vm",
        "headers": {
          "Host": ["www.bing.com"]
        }
      },
      "tls": {
        "enabled": true,
        "server_name": "www.bing.com",
        "insecure": false,
        "utls": {
          "enabled": true,
          "fingerprint": "chrome"
        }
      }
    },
    {
      "type": "hysteria2",
      "tag": "11",
      "server": "88.88.88.88",
      "server_port": 3443,
      "password": "2d1fe982-1323-4023-8322-069408455f32",
      "tls": {
        "enabled": true,
        "server_name": "www.bing.com",
        "insecure": true,
        "alpn": ["h3"]
      }
    },
    {
      "type": "tuic",
      "tag": "12",
      "server": "88.88.88.88",
      "server_port": 4443,
      "uuid": "2d1fe982-1323-4023-8322-069408455f32",
      "password": "2d1fe982-1323-4023-8322-069408455f32",
      "congestion_control": "bbr",
      "udp_relay_mode": "native",
      "udp_over_stream": false,
      "zero_rtt_handshake": false,
      "heartbeat": "10s",
      "tls": {
        "enabled": true,
        "server_name": "www.bing.com",
        "insecure": true,
        "alpn": ["h3"]
      }
    },
    {
      "tag": "fallback",
      "type": "direct"
    },
    {
      "tag": "direct",
      "type": "direct"
    },
    {
      "tag": "auto",
      "type": "urltest",
      "outbounds": [
        "10",
        "11",
        "12"
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "1m",
      "tolerance": 50,
      "interrupt_exist_connections": false
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "rule_set": [
      {
        "tag": "geosite-geolocation-!cn",
        "type": "remote",
        "format": "binary",
        "url": "https://cdn.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/geolocation-!cn.srs",
        "download_detour": "select",
        "update_interval": "1d"
      },
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://cdn.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/geolocation-cn.srs",
        "download_detour": "select",
        "update_interval": "1d"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://cdn.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/cn.srs",
        "download_detour": "select",
        "update_interval": "1d"
      }
    ],
    "final": "select",
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "port": 53,
        "action": "hijack-dns"
      },
      {
        "port": 443,
        "network": "udp",
        "action": "reject"
      },
      {
        "clash_mode": "Direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "Global",
        "outbound": "select"
      },
      {
        "rule_set": "geoip-cn",
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-cn",
        "outbound": "direct"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "outbound": "select"
      }
    ]
  }
}
